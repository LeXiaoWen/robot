<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/dp_32"
    android:layout_marginBottom="14dp"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <!--<include layout="@layout/fg_video_item"/>-->



        <LinearLayout
        android:layout_width="251dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/iv1"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:src="@drawable/sel_stri_init"/>

        <ImageView
            android:id="@+id/iv2"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/dp_1"
            android:src="@drawable/sel_stri_clamping"/>

        <ImageView
            android:id="@+id/iv3"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/dp_1"
            android:src="@drawable/sel_stri_clamp_closed"/>

        <ImageView
            android:id="@+id/iv4"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/dp_1"
            android:src="@drawable/sel_stri_peel"/>

        <ImageView
            android:id="@+id/iv5"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/dp_1"
            android:src="@drawable/sel_stri_cut_off"/>

        <ImageView
            android:id="@+id/iv6"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/dp_1"
            android:src="@drawable/sel_stri_unclock"/>

        <ImageView
            android:id="@+id/iv7"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/dp_1"
            android:src="@drawable/sel_stri_state_reset"/>

        <ImageView
            android:id="@+id/iv8"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/dp_1"
            android:src="@drawable/sel_stri_stop"/>


    </LinearLayout>

        <RelativeLayout
            android:id="@+id/rl_main"
            android:layout_width="672dp"
            android:layout_height="377dp"
            android:layout_marginLeft="@dimen/dp_15"
            android:background="@color/black"/>

        <RelativeLayout
            android:layout_marginTop="2dp"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_below="@id/rl_main"
            android:id="@+id/rl1"
            android:layout_width="167dp"
            android:layout_height="111dp"
            android:background="@color/black">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:padding="@dimen/dp_6"
                android:text="位姿仿真画面"
                android:textColor="@color/cmvp_white"
                android:textSize="@dimen/sp_8"
                android:textStyle="bold"
                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="2dp"
            android:layout_toRightOf="@id/rl1"
            android:layout_below="@id/rl_main"
            android:id="@+id/rl2"
            android:layout_width="167dp"
            android:layout_height="111dp"
            android:layout_marginLeft="@dimen/dp_2"
            android:background="@color/black">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:padding="@dimen/dp_6"
                android:text="位姿仿真画面"
                android:textColor="@color/cmvp_white"
                android:textSize="@dimen/sp_8"
                android:textStyle="bold"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="2dp"
            android:layout_toRightOf="@id/rl2"
            android:layout_below="@id/rl_main"
            android:id="@+id/rl3"
            android:layout_width="167dp"
            android:layout_height="111dp"
            android:layout_marginLeft="@dimen/dp_2"
            android:background="@color/black">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:padding="@dimen/dp_6"
                android:text="位姿仿真画面"
                android:textColor="@color/cmvp_white"
                android:textSize="@dimen/sp_8"
                android:textStyle="bold"
                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="2dp"
            android:layout_toRightOf="@id/rl3"
            android:layout_below="@id/rl_main"
            android:id="@+id/rl4"
            android:layout_width="167dp"
            android:layout_height="111dp"
            android:layout_marginLeft="@dimen/dp_2"
            android:background="@color/black">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:padding="@dimen/dp_6"
                android:text="位姿仿真画面"
                android:textColor="@color/cmvp_white"
                android:textSize="@dimen/sp_8"
                android:textStyle="bold"
                />
        </RelativeLayout>

