<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <!--<include layout="@layout/fg_video_item"/>-->

    <RelativeLayout
        android:layout_marginBottom="11dp"
        android:layout_marginTop="35dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/rl_main"
            android:layout_width="672dp"
            android:layout_height="377dp"
            android:layout_marginLeft="@dimen/dp_15"
            android:background="@color/black">
            <include layout="@layout/web_error_main"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:padding="@dimen/dp_6"
                    android:text="行线画面"
                    android:textColor="@color/cmvp_white"
                    android:textSize="@dimen/sp_8"
                    android:textStyle="bold"
            />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="672dp"
            android:layout_height="111dp"
            android:layout_below="@id/rl_main"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_2"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/rl1"
                android:layout_width="167dp"
                android:layout_height="111dp"
                android:background="@color/black">
                <include layout="@layout/web_error1"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:padding="@dimen/dp_6"
                    android:text="云台画面"
                    android:textColor="@color/cmvp_white"
                    android:textSize="@dimen/sp_8"
                    android:textStyle="bold"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl2"
                android:layout_width="167dp"
                android:layout_height="111dp"
                android:layout_marginLeft="@dimen/dp_2"
                android:background="@color/black">
                <include layout="@layout/web_error2"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:padding="@dimen/dp_6"
                    android:text="引流线画面"
                    android:textColor="@color/cmvp_white"
                    android:textSize="@dimen/sp_8"
                    android:textStyle="bold"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl3"
                android:layout_width="167dp"
                android:layout_height="111dp"
                android:layout_marginLeft="@dimen/dp_2"
                android:background="@color/black">
                <include layout="@layout/web_error3"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:padding="@dimen/dp_6"
                    android:text="主臂画面"
                    android:textColor="@color/cmvp_white"
                    android:textSize="@dimen/sp_8"
                    android:textStyle="bold"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl4"
                android:layout_width="167dp"
                android:layout_height="111dp"
                android:layout_marginLeft="@dimen/dp_2"
                android:background="@color/black">
                <include layout="@layout/web_error4"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:padding="@dimen/dp_6"
                    android:text="从臂画面"
                    android:textColor="@color/cmvp_white"
                    android:textSize="@dimen/sp_8"
                    android:textStyle="bold"
                    />
            </RelativeLayout>

        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_marginBottom="71dp"
        android:layout_marginTop="31dp"
        android:layout_width="251dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/iv1"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_1"/>

        <ImageView
            android:id="@+id/iv2"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/jiexian_jiting_normal"/>

        <ImageView
            android:id="@+id/iv3"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_3"/>

        <ImageView
            android:id="@+id/iv4"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_4"/>

        <ImageView
            android:id="@+id/iv5"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_5"/>

        <ImageView
            android:id="@+id/iv6"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_6"/>

        <ImageView
            android:id="@+id/iv7"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_7"/>

        <ImageView
            android:id="@+id/iv8"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_8"/>

        <ImageView
            android:id="@+id/iv9"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_9"/>

        <ImageView
            android:id="@+id/iv10"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:src="@drawable/sel_jie_10"/>




    </LinearLayout>


</RelativeLayout>